{
  "name": "topic-tailer",
  "version": "1.0.0",
  "description": "A simple tool to stream topics to the browser",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "export UV_THREADPOOL_SIZE=32 && npm run build && node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "export UV_THREADPOOL_SIZE=32 && nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "prepare": "patch-package"
  },
  "author": "Tom Underhill",
  "dependencies": {
    "axios": "^0.30.0",
    "bootstrap": ">=4.3.1",
    "express": "^4.16.3",
    "kafka-avro": "^1.1.3",
    "patch-package": "^5.1.1",
    "postinstall-prepare": "^1.0.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-modal": "^3.8.1",
    "react-rnd": "^9.0.4",
    "react-sliding-pane": "^3.1.0",
    "react-table": "^6.8.6",
    "request": "^2.88.0",
    "socket.io": "^2.2.0",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@babel/core": "7.3.4",
    "@babel/plugin-proposal-class-properties": "7.3.4",
    "@babel/preset-env": "7.3.4",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "clean-webpack-plugin": "1.0.1",
    "concurrently": "4.1.0",
    "css-loader": "2.1.0",
    "eslint": "5.15.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "html-webpack-plugin": "3.2.0",
    "nodemon": "1.18.10",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3",
    "webpack-dev-server": "3.2.1"
  }
}
